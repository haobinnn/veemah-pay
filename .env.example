DATABASE_URL='postgresql://username:password@host:port/database?sslmode=require&channel_binding=require'

# Cleaned PostgreSQL URL for Java server (automatically extracted from DATABASE_URL)
CLEANED_POSTGRESS_URL='jdbc:postgresql://host:port/database'

# Enable Java Transaction Server
USE_JAVA_TRANSACTIONS=true

# Java Transaction Server URL for client-side (must use NEXT_PUBLIC for browser access)
NEXT_PUBLIC_JAVA_API_URL=https://your-production-domain.com

# Server-side Java Transaction Server URL (fallback)
JAVA_TRANSACTION_API=https://your-production-domain.com